opr <=(a:ZZ32, b:ZZ32):Boolean = builtinPrimitive("com.sun.fortress.interpreter.glue.prim.Int$LessEq")
opr MIN(a:ZZ32, b:ZZ32):ZZ32 = if a <= b then a else b end
opr MAX(a:ZZ32, b:ZZ32):ZZ32 = if b <= a then a else b end
