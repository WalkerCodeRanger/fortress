component restTest2
export Executable


foo(y:ZZ32, z:ZZ32, x:ZZ32...) = do
   var sum:ZZ32 = y + z
   for i <- x.base()#x.size() do
     sum += x[i]
   end
   sum
end

foo(y:ZZ32) = y


run(args:String...):() = do
   println(foo(1,2,3,4,5))
   println(foo(17))
end

end
