component fortress.typecaseTest.fss
export Executable

run(args:String...):()=do
   typecase x = 5 of
      String => print("fail ")
      ZZ32 => print("pass ")
      else => print("fail ")
   end

   typecase y = "One Two Three " of
      String => print("pass ")
      ZZ32 => print("fail ")
      else => print("fail ")
   end

   typecase z = 3.1415 of
      String => print("fail ")
      ZZ32 => print("fail ")
      else => print("pass ")
   end

   typecase zz = 3.1415 of
      String => print("fail ")
      ZZ32 => print("fail ")
      Number => print("pass ")
   end

   typecase (a = 1, b = 3.1415, c = "Hurrah") of
	(Number, Number, Number) => print("fail ")
	(Number, Number, String) => print("pass ")
   end

end
end
